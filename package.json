{
	"name": "@nl-design-system-community/nlds-community-blocks",
	"version": "1.0.0",
	"description": "WordPress Gutenberg Blocks for NL Design System components. Use this so authors of WordPress websites can use NL Design System components to compose their pages.",
	"license": "EUPL-1.2",
	"author": "Acato",
	"main": "build/index.js",
	"keywords": [
		"nl-design-system",
		"wordpress",
		"wordpress-plugin"
	],
	"contributors": [
		{
			"name": "Paul van Impelen",
			"email": "paul@acato.nl",
			"url": "https://acato.nl"
		},
		{
			"name": "Richard Korthuis",
			"email": "richardkorthuis@acato.nl",
			"url": "https://acato.nl"
		},
		{
			"name": "Yolijn van der Kolk",
			"email": "yolijn@frameless.io"
		},
		{
			"name": "Robbert Broersma",
			"email": "robbert@frameless.io"
		}
	],
	"engines": {
		"node": "^20",
		"pnpm": "^8"
	},
	"scripts": {
		"build": "wp-scripts build src/blocks/index.js && mix build --production && wp-scripts plugin-zip",
		"build-assets": "mix build --production",
		"format": "npm-run-all --sequential format:** prettier",
		"format:css": "stylelint --allow-empty-input --fix '**/*.{css,scss}'",
		"format:js": "eslint --ext '.js,.jsx' --fix --report-unused-disable-directives .",
		"lint": "npm-run-all lint:**",
		"lint:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
		"lint:js": "eslint --ext '.js,.jsx' --report-unused-disable-directives .",
		"lint:package-json": "npmPkgJsonLint '**/package.json'",
		"lint:prettier": "prettier --check .",
		"prettier": "prettier --write .",
		"start": "wp-scripts start src/blocks/index.js",
		"start-assets": "mix watch",
		"packages-update": "wp-scripts packages-update",
		"test-update": "pnpm run --if-present lint && pnpm run --if-present test && pnpm run --if-present build && pnpm run --if-present lint-build && pnpm run --if-present test-build",
		"update-patch": "npm-check-updates --configFileName .ncurc.patch.cjs --deep --dep dev,prod --target patch --upgrade && pnpm install",
		"update-minor": "npm-check-updates --configFileName .ncurc.minor.cjs --deep --dep dev,prod --target minor --upgrade && pnpm install",
		"update-major": "npm-check-updates --configFileName .ncurc.major.cjs --deep --dep dev,prod --target latest --upgrade && pnpm install"
	},
	"devDependencies": {
		"@wordpress/eslint-plugin": "17.7.0",
		"@wordpress/prettier-config": "3.7.0",
		"@wordpress/scripts": "27.1.0",
		"@wordpress/stylelint-config": "21.33.0",
		"eslint": "8.56.0",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-react": "7.33.2",
		"laravel-mix": "6.0.49",
		"npm-check-updates": "16.14.14",
		"npm-package-json-lint": "7.1.0",
		"npm-run-all": "4.1.5",
		"resolve-url-loader": "5.0.0",
		"stylelint": "16.2.1",
		"stylelint-config-standard-scss": "13.0.0",
		"wp-prettier": "3.0.3"
	},
	"dependencies": {
		"@gemeente-denhaag/accordion": "0.2.3-alpha.371",
		"@gemeente-denhaag/blockquote": "0.1.1-alpha.282",
		"@gemeente-denhaag/button": "0.2.3-alpha.371",
		"@gemeente-denhaag/button-group": "0.1.1-alpha.240",
		"@gemeente-denhaag/components-css": "0.1.1-alpha.319",
		"@gemeente-denhaag/descriptionlist": "0.1.1-alpha.271",
		"@gemeente-denhaag/design-tokens-common": "0.2.3-alpha.371",
		"@gemeente-denhaag/design-tokens-components": "0.2.3-alpha.371",
		"@gemeente-denhaag/divider": "0.2.3-alpha.371",
		"@gemeente-denhaag/highlighted-links": "0.1.1-alpha.194",
		"@gemeente-denhaag/icons": "0.2.3-alpha.371",
		"@gemeente-denhaag/image": "0.1.1-alpha.280",
		"@gemeente-denhaag/link": "0.2.3-alpha.371",
		"@gemeente-denhaag/link-group": "0.1.3-alpha.232",
		"@gemeente-denhaag/note": "0.1.1-alpha.284",
		"@gemeente-denhaag/table": "0.1.1-alpha.273",
		"@gemeente-denhaag/typography": "0.2.3-alpha.371",
		"@wordpress/block-editor": "12.18.2",
		"@wordpress/blocks": "12.27.1",
		"@wordpress/components": "25.16.0",
		"@wordpress/data": "9.20.0",
		"@wordpress/element": "5.27.0",
		"@wordpress/icons": "9.41.0",
		"classnames": "2.5.1",
		"is-url": "1.2.4"
	}
}
