{
  "title": "Image",
  "description": "Image with caption and/or with download link.",
  "icon": "format-image",
  "name": "ncb-denhaag/image",
  "category": "nlds-community-denhaag-blocks",
  "textdomain": "nlds-community-blocks",
  "attributes": {
    "caption": {
      "type": "string",
      "default": null
    },
    "download": {
      "type": "boolean",
      "default": false
    },
    "hasCaption": {
      "type": "boolean",
      "default": false
    },
    "image": {
      "type": "number",
      "default": 0
    },
    "size": {
      "type": "string",
      "default": "full"
    }
  },
  "keywords": [
    "denhaag",
    "image",
    "download",
    "caption"
  ],
  "supports": {
    "className": false,
    "customClassName": false,
    "html": false
  },
  "style": [
    "ncb-denhaag-image-block",
    "ncb-denhaag-divider-block"
  ],
  "editorStyle": [
    "ncb-denhaag-image-block-editor"
  ],
  "version": "2.0.0",
  "variations": [
    {
      "name": "ncb-denhaag/image-caption",
      "title": "Image with caption",
      "attributes": {
        "hasCaption": true
      },
      "example": {
        "attributes": {
          "hasCaption": true
        }
      }
    },
    {
      "name": "ncb-denhaag/image-download",
      "title": "Image with download",
      "attributes": {
        "download": true
      },
      "example": {
        "attributes": {
          "download": true
        }
      }
    },
    {
      "name": "ncb-denhaag/image-caption-download",
      "title": "Image with caption and download",
      "attributes": {
        "download": true,
        "hasCaption": true
      },
      "example": {
        "attributes": {
          "download": true,
          "hasCaption": true
        }
      }
    }
  ]
}