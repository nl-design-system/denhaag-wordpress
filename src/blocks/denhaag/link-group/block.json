	"supports": {
		"className": false,
		"customClassName": false,
		"html": false
	},
	"style": ["ncb-denhaag-link-group-block", "ncb-denhaag-heading-block"],
	"editorStyle": [
		"ncb-denhaag-heading-block-editor",
		"ncb-denhaag-link-group-block-editor"
