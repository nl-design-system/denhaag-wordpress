{
	"title": "Link group",
	"description": "A group of links with a heading and optional heading.",
	"icon": "editor-justify",
	"name": "ncb-denhaag/link-group",
	"category": "nlds-community-denhaag-blocks",
	"attributes": {
		"level": {
			"type": "number",
			"default": 4,
			"enum": [2, 3, 4]
		},
		"caption": {
			"type": "string",
			"default": ""
