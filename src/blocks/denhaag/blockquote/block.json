{
  "title": "Blockquote",
  "description": "A quote component.",
  "icon": "format-quote",
  "name": "ncb-denhaag/blockquote",
  "category": "nlds-community-denhaag-blocks",
  "attributes": {
	"quote": {
	  "type": "string",
	  "source": "text",
	  "selector": "p"
	},
	"link": {
	  "type": "object"
	},
	"author": {
	  "type": "string",
	  "source": "text",
	  "selector": "figcaption"
	},
	"hasAuthor": {
	  "type": "boolean",
	  "default": false
	}
  },
  "example": {
	"attributes": {
	  "author": "Nelson Mandela",
	  "hasAuthor": true,
	  "quote": "The greatest glory in living lies not in never falling, but in rising every time we fall."
	}
  },
  "supports": {
	"className": false,
	"customClassName": false,
	"html": false
  },
  "style": [
	"ncb-denhaag-blockquote-block"
  ],
  "version": "2.0.0",
  "keywords": [
	"cite",
	"citation",
	"denhaag",
	"quote"
  ]
}
